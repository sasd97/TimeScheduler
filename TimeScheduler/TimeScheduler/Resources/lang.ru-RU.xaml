<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">

    <v:String x:Key="AppTitle">Расписание</v:String>
    <v:String x:Key="AppLanguages">Языки</v:String>

    <!-- Region Toast -->
    <v:String x:Key="StateChanged">Timer state changed: {0}</v:String>
    <v:String x:Key="TimerStopped">Timer seccessfully stopped</v:String>
    <v:String x:Key="TimerLow">Prepare to changing state</v:String>
    <!-- Endregion -->

    <!-- Region About text -->
    <v:String x:Key="SendMail">Отправить письмо</v:String>
    <v:String x:Key="GoToGithub">На github</v:String>
    <v:String x:Key="AboutApplication">1</v:String>
    <v:String x:Key="AboutAuthor">1</v:String>
    <!-- Endregion -->

    <!-- Region App Info Text -->
    <v:String x:Key="InfoWorkText">Установи рабочее время</v:String>
    <v:String x:Key="InfoRestText">Установи время отдыха</v:String>
    <!-- Endregion -->

    <!-- Region App Navigation -->
    <v:String x:Key="NavigationLanguges">Языки</v:String>
    <v:String x:Key="NavigationAbout">О приложении</v:String>
    <!-- Endregion -->

    <!-- Region App Controls -->
    <v:String x:Key="StateWork">Работа</v:String>
    <v:String x:Key="StateRest">Отдых</v:String>
    <v:String x:Key="StateOff">Остановлено</v:String>
    <v:String x:Key="ControlEnableNotification">Включить уведомления</v:String>
    <v:String x:Key="ControlEnableNotification_Enabled">Включено</v:String>
    <v:String x:Key="ControlEnableNotification_Disabled">Выключено</v:String>
    <v:String x:Key="ControlStart">Старт</v:String>
    <v:String x:Key="ControlStop">Стоп</v:String>
    <v:String x:Key="ControlPause">Пауза</v:String>
    <v:String x:Key="ControlResume">Продолжить</v:String>
    <!-- Endregion -->

    <!-- Region Author Flyout -->
    <v:String x:Key="AboutFloyoutHeader">О приложении "Time Scheduler"</v:String>
    <!-- Endregion -->

    <!-- Region Tabs -->
    <v:String x:Key="TabTimerTitle">Таймер</v:String>
    <v:String x:Key="TabActivityTitle">Активность</v:String>
    <!-- Endregion -->

    <!-- Region Timer Tab -->
    <v:String x:Key="ElapsedTime">Прошло</v:String>
    <v:String x:Key="LeftTime">Осталось</v:String>
    <v:String x:Key="TimePlaceholder">Таймер выключен!</v:String>
    <!-- Endregion -->

    <!-- Region Activity Tab -->
    <v:String x:Key="ColumnData">Дата</v:String>
    <v:String x:Key="ColumnTime">Время</v:String>
    <v:String x:Key="ColumnType">Тип</v:String>
    <!-- Endregion -->

    <!-- Region Resume Dialog -->
    <v:String x:Key="ResumeDialogTitle">Пауза</v:String>
    <v:String x:Key="ResumeDialogBody">Для возобновления нажмите кнопку 'OK'</v:String>
    <!-- Endregion -->
</ResourceDictionary>